<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="imei_generation_lines_seq" model="ir.sequence">
        <field name="name">Serial Number</field>
        <field name="code">serial_sq</field>
        <field name="prefix">VIM125607</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
        </record>
    </data>
    <data>
        <record id="imei_generation_seq" model="ir.sequence">
        <field name="name">Name Sequence</field>
        <field name="code">name_sq</field>
        <field name="prefix">%(year)s/%(day)s</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>